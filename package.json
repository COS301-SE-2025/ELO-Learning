{
  "devDependencies": {
    "eslint": "^9.27.0",
    "jest": "^29.7.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "lint:check": "eslint . --ext .js --quiet",
    "lint:check:fix": "eslint . --ext .js --quiet --fix",
    "lint:check:fix:all": "eslint . --ext .js --quiet --fix --ignore-pattern node_modules",
    "test": "jest --passWithNoTests"
  }
}
