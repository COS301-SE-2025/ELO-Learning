{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["frontend/src/*"]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["frontend/**/*"],
  "exclude": [
    "node_modules",
    "backend",
    "coverage",
    "cypress",
    ".next",
    "out",
    "build",
    "dist"
  ]
}
